{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "network-device-id": {
      "type": "string",
      "description": "The unique identifier for the Device. Mac-address is used",
      "pattern":"^([0-9A-Fa-f]{2}[:]){5}([0-9A-Fa-f]{2})$"
    },
    "endpoint-id": {
      "type": "string",
      "description": "The unique identifier for the Device. Mac-address is used",
      "pattern":"^([0-9A-Fa-f]{2}[:]){5}([0-9A-Fa-f]{2})$"
    },
    "network-interface": {
      "type": "string"
    },
    "endpoint-port-id": {
      "type": "string"
    },
    "status": {
      "enum": [ "up","down" ]
    },
    "speed": {
      "enum": [ "100M","1G","10G","25G","40G","50G","100G" ]
    }
  },
  "required": [
    "network-device-id",
    "endpoint-id",
    "network-interface",
    "endpoint-port-id",
    "status",
    "speed"
  ]
}
