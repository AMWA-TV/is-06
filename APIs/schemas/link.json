{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "device_id": {
      "type": "string"
    },
    "network_interface": {
      "type": "string"
    },
    "peer_device_id": {
      "type": "string"
    },
    "peer_network_interface": {
      "type": "string"
    },
    "speed": {
      "enum": [ "100M","1G","10G","25G","40G","50G","100G" ]
    }
  },
  "required": [
    "device_id",
    "network_interface",
    "peer_device_id",
    "peer_network_interface",
    "speed"
  ]
}
